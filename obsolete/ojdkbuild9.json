                "extract_dir": "java-$matchShort-openjdk-$version$matchTail.x86_64"
            },
            "32bit": {
                "url": "https://github.com/ojdkbuild/ojdkbuild/releases/download/$version/java-$matchShort-openjdk-$version$matchTail.x86.zip",
